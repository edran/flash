{
  "manifest_version": 2,
  "name": "Jelly",
  "version": "0.1",
  "description": "A Chrome extension to automatically create flashcards",
  "background":{
    "scripts":["js/jquery-3.3.1.min.js", "js/background.js"]
  },
  "content_scripts": [
    {
      "matches":[
        "<all_urls>"
      ],
      "js": ["js/content.js", "js/jquery-3.3.1.min.js"]
    }
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "html/popup.html"
  },
  "web_accessible_resources": [
    "img/icon.png",
    "img/anki_logo.jpg",
    "html/card-template.html",
    "html/notification.html",
    "html/anki/text-fb-card.html"
  ],
  "permissions": [
    "storage"
  ]
}
