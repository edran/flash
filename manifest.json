{
  "manifest_version": 2,
  "name": "Jelly",
  "version": 0.1,
  "description": "A Chrome extension to automatically create flashcards",
  "background":{
    "scripts":["js/background.js"]
  },
  "content_scripts": {
    "matches":[
      "<all_urls>"
    ],
    "js": ["js/content.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "html/popup.html"
  }
}
